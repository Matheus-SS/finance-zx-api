{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "src",
  "compilerOptions": {
    "deleteOutDir": true,
    "builder": "swc",
    "typeCheck": false,
    "assets": [
      {
        "include": "/extra/localization/i18n/**/*", "watchAssets": true
      }
    ]
  }
}
